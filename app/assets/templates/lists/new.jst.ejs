<h2>Create a New List</h2>

<form id="new-list">
  <input type="hidden" name="list[board_id]" value="<%= board.escape("id") %>">
  <input type="hidden" name="list[rank]" value="<%= board.lists().length+1 %>">
  <label>
    List Title:
    <input type="text" name="list[title]">
  </label>
  
  <input type="submit" value="Create List">
</form>